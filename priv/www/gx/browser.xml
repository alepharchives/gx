<?xml version="1.0"?>
<gxml>
	<dialog id="browser" title="GXML Browser">
		<panel>
			<panel layout="row" fill="true">
				<text border="5">URL:</text>
				<entry id="url" border="2" fill="width" onselect="on_browse">http://localhost:8000/gx/minimal.xml</entry>
				<button label="Go!" border="1" onclick="on_click"/>
			</panel>
			<image align="center" />
			<button label="Close" border="5" align="center" onclick="on_close"/>
		</panel>
	</dialog>
</gxml>
	
