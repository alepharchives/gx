<gx module="?maybe?" onerror="error">
	<window id="win" pos="center" title="GX Tutorial" onunload="on_exit">
		<!-- 
		-->
		<statusbar>Welcome to GX</statusbar>
		<panel>
			<button id="about_button" fill="true" border="10" label="About..." onclick="on_about" />
			<text border="10" align="center">This is a text label</text>
			<checkbox label="test a checkbox" border="10" onselect="on_message" />
			<radiobutton label="test a radiobutton" border="10" onselect="on_message" />
		</panel>
	</window>
</gxml>
