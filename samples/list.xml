<?xml version="1.0"?>
<gxml>
	<window id="win" title="Example - List" onunload="on_close">
		<panel>
			<list label="Priority" fill="true" border="5" onselect="on_message">
				<item label="First"/>
				<item label="Hidden">Second</item>
				<item>Third</item>
