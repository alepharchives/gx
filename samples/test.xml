<?xml version="1.0"?>
<gx>
	<window id="win" 
			title="GXML TEST" 
			icon="feed.png" 
			width="600" 
			height="400"
			onload="init">
		<menubar id="menus">
			<menu id="file" label="File">
				<menuitem label="&amp;New" command="5002"/>
				<menuitem label="&amp;Open" command="5000"/>
				<separator />
				<menuitem label="&amp;Quit" command="5006"/>
			</menu>
			<menu id="help" label="Help">
				<menuitem label="&amp;Contents" enable="false" command="5015"/>
				<separator/>
				<menuitem label="&amp;About..." accelerator="F1" command="5014"/>
			</menu>
		</menubar>
		<!--toolbar>
			<tool id="new" label="New" icon="wxe.xpm" command="5002"/>
		</toolbar-->
		<tabs id="tabs"
			position="top">
			<editor id="stc1" title="Editor" lexer="Erlang"/>
			<editor id="stc2" title="Editor2" lexer="Erlang"/>
		</tabs>
		<statusbar text="Ready"/>
	</window>
</gx>
