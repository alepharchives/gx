<?xml version="1.0"?>
<gxml>
	<frame title="Example: combobox" onunload="on_close">
		<statusbar/>
		<panel>
			<combo value="Priority" fill="true" border="5" onchange="on_message" onselect="on_message">
				<item>First</item>
				<item>Second</item>
				<item label="Third"/>
			</combo>
			<button align="center" border="5" onclick="on_message"/>
		</panel>
	</frame>
</gxml>
